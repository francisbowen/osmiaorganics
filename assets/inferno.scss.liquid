      z-index: 1;
      font-size: 17px;
      color: #676767;
      font-weight: bold;
      text-transform: uppercase;
      padding: 15px 0;
    }
  
    &.current_selection {
      
      &:before {
        border-color: $color-btn-primary;
      }
      
      span {
        color: $color-btn-primary;
      }
    }
    
    &.adjusted {
      
      &:after {
        background: #a7a7ae;
      }
      
      span {
        color: #676767;
      }
  
      &.current_selection {
        
        &:after {
          background: rgba($color-btn-primary,.4);
        }
      }
    }
  }
