.question-block {
  @include transition(all ease-out 0.35s);

  &.active {
    opacity: 1;
    position: relative;
  }
}

.question-block--answer {
  position: relative;
  -webkit-box-flex: 0;
  -webkit-flex: 0 0 33.333333%;
  -ms-flex: 0 0 33.333333%;
  flex: 0 0 33.333333%;
  max-width: 33.333333%;
  padding-left: 10px;
  padding-right: 10px;
  padding-bottom: 20px;
  
  &:before {
    content: '';
    display: block;
    padding-top: 80%;
    background-size: cover;
    background-position: center;
    background-repeat: no-repeat;
    border: 2px solid $color-body;
    @include transition(all ease-out 0.35s);
  }
  
  &:after {
    content: '';
    display: block;
    position: absolute;
    top: 0;
    right: 10px;
    bottom: 20px;
    left: 10px;
    background: rgba(#ceced6,.4);
    mix-blend-mode: multiply;
  }
  
  span {
    position: absolute;
    left: 20px;
    bottom: 30px;
    right: 20px;
    color: $color-body-text;
    text-transform: uppercase;
    z-index: 1;
  }

  &.current_selection {
    
    &:before {
      border-color: $color-btn-primary;
    }
    
    span {
      color: $color-btn-primary;
    }
  }
  
  &.adjusted {
    
    &:after {
      background: #a7a7ae;
    }
    
    span {
      color: $color-body;
    }

    &.current_selection {
      
      &:after {
        background: rgba($color-btn-primary,.4);
      }
    }
  }
}

.question_level {
  list-style: none;
  margin: 0;
  padding: 0;
  display: block;
  
  li {
    cursor: pointer;
    display: inline-block;
    margin: 5px;
    
    &:before {
      content: '';
      display: block;
      width: 12px;
      height: 12px;
      border-radius: 7px;
      background: #ceced6;
      border: 1px solid #ceced6;
    }
    
    &:last-of-type {
      pointer-events: none;
    
      &:before {
        background: $color-btn-primary;
      }
    }
  }
}

