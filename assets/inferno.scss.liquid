  a {
    color: inherit;
    text-decoration: none;
    font-weight: inherit;
  }
}

h1 {
  font-size: $headline1-size;
  text-transform: $headline1-case;
  font-weight: $headline1-weight;
  letter-spacing: $headline1-ls;
  @include media-query($max,$sm) {
